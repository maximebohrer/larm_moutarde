<launch>
    <!-- <param name="/use_sim_time" value="true"/> -->

    <!-- rviz -->
    <node name="rviz" pkg="rviz" type="rviz" args="-d $(find slam)/src/fichier_rviz.rviz" />

    <!-- gmapping -->
    <node name="gmapping" pkg="gmapping" type="slam_gmapping" args="scan:=base_scan" />

    <!-- rosbag -->
    <node name="rosbag" pkg="rosbag" type="play" args="--clock $(find slam)/src/turtlebot_dia.bag">
        <remap from="scan" to="base_scan"/>
    </node>
</launch>