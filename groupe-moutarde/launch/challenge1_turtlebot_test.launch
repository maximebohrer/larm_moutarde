<launch>
    <!-- lancer le robot -->
    <include file="$(find turtlebot_bringup)/launch/minimal.launch" />

    <!-- lancer le laser - publication dans /scan -->
    <node name="laser" pkg="urg_node" type="urg_node" />

    <!-- contrÃ´le du robot -->
    <node name="controle" pkg="groupe-moutarde" type="teleoptest.py">
        <remap from="/cmd_vel" to="/cmd_vel_mux/input/navi" />
    </node>

    <!-- rviz -->
    <node name="rviz" pkg="rviz" type="rviz" args="-d $(find groupe-moutarde)/launch/fichier_rviz.rviz" />
</launch>