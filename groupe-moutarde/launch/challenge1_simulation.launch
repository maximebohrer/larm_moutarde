<launch>
    <!-- démarrer la simulation dans Gazebo et ajouter un robot -->
    <include file="$(find larm)/launch/challenge-1.launch" />

    <!-- contrôle du robot -->
    <node name="controle" pkg="groupe-moutarde" type="teleop2.py" />

    <!-- rviz -->
    <node name="rviz" pkg="rviz" type="rviz" args="-d $(find groupe-moutarde)/launch/fichier_rviz.rviz" />
    <node name="rviz2" pkg="rviz" type="rviz" args="-d $(find groupe-moutarde)/launch/fichier_rviz2.rviz" />
</launch>
